    "license": "MIT",
    "authors": [
        {
            "name": "Arslanoov",
            "email": "rasularslanoov@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm"
    },
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "acclimate/container": "^2.0",
        "aura/router": "^3.1",
        "doctrine/migrations": "^3.0",
        "doctrine/orm": "^2.8",
        "laminas/laminas-config-aggregator": "^1.4",
        "laminas/laminas-httphandlerrunner": "^1.3",
        "northwoods/broker": "^3.0",
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1.0",
        "php-di/php-di": "^6.3",
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "doctrine/dbal": "^2.10",
        "finesse/swiftmailer-defaults-plugin": "^2.0",
        "laminas/laminas-diactoros": "^2.3",
        "laminas/laminas-servicemanager": "^3.4",
        "league/oauth2-server": "^8.1",
        "monolog/monolog": "^2.0",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1",
        "ramsey/uuid": "^4.0",
        "sentry/sdk": "^3.1",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/validator": "^5.1",
        "twig/twig": "^3.0",
        "webmozart/assert": "^1.9",
        "zircote/swagger-php": "3.0.4"
    },
    "require-dev": {
        "overtrue/phplint": "^2.2",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.1",
        "doctrine/data-fixtures": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/Application",
            "Domain\\": "src/Domain",
            "Framework\\": "src/Framework",
            "Infrastructure\\": "src/Infrastructure",
            "Tests\\": "tests",
            "Frontend\\": "src/Frontend"
        }
    }
}
